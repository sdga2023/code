{
  "name": "sdg2022",
  "version": "0.0.1",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.14.0"
  },
  "scripts": {
    "dev": "vite dev --host",
    "build": "./production.sh production",
    "package": "vite package",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "convert-tokens": "token-transformer src/design/tokens/tokens.json src/design/temp/temp_styles.json base,global mobile,fluid,typography --expandTypography=true --expandShadow=true --preserveRawValue=true",
    "build-styles": "npm run convert-tokens; node tasks/build-styles.js",
    "prettier": "prettier --write 'src/**/*.svelte'",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@googleapis/docs": "^1.0.1",
    "@googleapis/drive": "^4.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.86",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.14.0",
    "@sveltejs/svelte-scroller": "^2.0.7",
    "@turf/area": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "archieml": "^0.5.0",
    "d3": "^7.6.1",
    "d3-collection": "^1.0.7",
    "d3-geo-voronoi": "^2.0.1",
    "d3-regression": "^1.3.10",
    "d3-sankey": "^0.12.3",
    "d3-shape": "^3.1.0",
    "google-spreadsheet": "^3.3.0",
    "googleapis": "^105.0.0",
    "json2csv": "^5.0.7",
    "layercake": "^7.0.0",
    "mapbox-gl": "2.9.2",
    "node-fetch": "^3.2.10",
    "patch-package": "^6.5.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.4",
    "style-dictionary": "^3.7.1",
    "svelte-preprocess": "^4.10.7",
    "token-transformer": "^0.0.25"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "autoprefixer": "^10.4.8",
    "classnames": "^2.3.2",
    "d3-array": "^3.2.0",
    "d3-force": "^3.0.0",
    "d3-geo-projection": "^4.0.0",
    "d3-scale": "^4.0.2",
    "fuzzy": "^0.1.3",
    "postcss": "^8.4.16",
    "simple-statistics": "^7.7.6",
    "svelte-fa": "^3.0.3",
    "svelte-i18n": "^3.4.0",
    "svelte-inview": "^3.0.1",
    "svelte-search": "^2.0.1",
    "svelte-typeahead": "^4.3.2",
    "tailwindcss": "^3.1.8",
    "three": "^0.144.0",
    "topojson-client": "^3.1.0"
  }
}
